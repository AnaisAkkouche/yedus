<p id="notice"><%= notice %></p>

<h1>Add songs</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Singer</th>
      <th>Link</th>
      <th>Emotion</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <p></p>
  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.title %></td>
        <td><%= song.singer %></td>
        <td><%= song.link %></td>
        <td><%= song.emotion %></td>
        <td><%= link_to 'Show', song %></td>
        <td><%= link_to 'Edit', edit_song_path(song) %></td>
        <td><%= link_to 'Destroy', song, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Song', new_song_path %>
